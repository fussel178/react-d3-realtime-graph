{
	"name": "d3-app-typescript",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"start": "react-scripts start",
		"build": "react-scripts build",
		"test": "react-scripts test",
		"style": "prettier --write package.json '**/*{.ts,.tsx,.js,.jsx,.json,.html,.css,.scss,.sass,.yaml,.yml,.md}'",
		"style:ci": "prettier --list-different package.json '**/*{.ts,.tsx,.js,.jsx,.json,.html,.css,.scss,.sass,.yaml,.yml,.md}'",
		"lint": "eslint --ext .js,.jsx,.ts,.tsx --ignore-pattern node_modules/ --no-error-on-unmatched-pattern src",
		"clean": "rimraf build",
		"eject": "react-scripts eject"
	},
	"engines": {
		"node": ">=14",
		"npm": ">=7"
	},
	"dependencies": {
		"d3": "^7.3.0",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"react-virtualized-auto-sizer": "^1.0.6"
	},
	"devDependencies": {
		"@testing-library/jest-dom": "^5.16.4",
		"@testing-library/react": "^12.1.4",
		"@testing-library/user-event": "^13.5.0",
		"@types/d3": "^7.1.0",
		"@types/jest": "^27.4.1",
		"@types/node": "^17.0.36",
		"@types/react": "^17.0.41",
		"@types/react-dom": "^18.0.2",
		"@types/react-virtualized-auto-sizer": "^1.0.1",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-prettier": "^4.0.0",
		"prettier": "2.6.2",
		"react-scripts": "5.0.0",
		"rimraf": "^3.0.2",
		"typescript": "^4.6.3"
	},
	"eslintConfig": {
		"extends": [
			"react-app",
			"react-app/jest",
			"plugin:prettier/recommended",
			"prettier"
		],
		"plugins": [
			"prettier"
		]
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	}
}
